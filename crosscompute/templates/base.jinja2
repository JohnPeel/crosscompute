<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
{%- for style_uri in style_uris %}
  <link rel="stylesheet" href="{{ style_uri }}">
{%- endfor %}
</head>
<body>
{%- block body_content %}
{{ content_html | safe }}
{% endblock -%}
{%- for script_uri in script_uris %}
<script src="{{ script_uri }}"></script>
{% endfor -%}
<script>
{%- block body_script %}
{{ script_text | safe }}
{%- endblock %}
</script>
</body>
</html>
