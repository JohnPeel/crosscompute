<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
{%- for style in styles %}
  <link rel="stylesheet" href="{{ style['uri'] }}">
{%- endfor -%}
</head>
<body>
{%- block body_content %}
{{ body_content | safe }}
{% endblock -%}
{%- for script in scripts %}
<script src="{{ script['uri'] }}"></script>
{% endfor -%}
<script>
{%- block body_script %}
{{ body_script }}
{% endblock -%}
</script>
</body>
</html>
