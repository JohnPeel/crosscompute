{% extends 'base.jinja2' if IS_STATIC else 'live.jinja2' %}

{% block header_html %}
<a href="{{ BASE_URI if BASE_URI else '/' }}">Root</a> &gt;
<a href="{{ BASE_URI }}{{ uri }}">{{ name }}</a>
{% endblock %}

{% block body_html %}
<h1>{{ name }}</h1>
<ul>
{%- for batch in batches %}
<li>{{ batch['name'] }} <a href="{{ BASE_URI }}{{ uri }}{{ batch['uri'] }}/o">[output]</a></li>
{%- endfor %}
</ul>
{% endblock %}
